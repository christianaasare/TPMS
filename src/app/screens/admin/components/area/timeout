 HC_exporting(Highcharts);

    function Last7Days () {
      var result = [];
      for (var i=0; i<7; i++) {
          var d = new Date();
          d.setDate(d.getDate() - i);
          result.push(d)
      }
  
      return(result.join(','));
  }

    setTimeout(() => {
      Last7Days()
    }, 5000);
    console.log("Timeout occurred");
  }); 